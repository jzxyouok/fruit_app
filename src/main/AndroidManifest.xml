<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.myapplication" >

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!--
            一个activity就是一个页面
        -->
        <activity
            android:name="com.naoh.Fruit.ui.LoadActivity">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter></activity>


        <activity
            android:name="com.naoh.Fruit.ui.MainActivity"

            android:theme="@style/AppTheme">
           <!--
           <intent-filter>
               <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            -->
        </activity>
        <activity android:name="com.naoh.Fruit.ui.home.DetailActivity" />
        <activity android:name="com.naoh.Fruit.ui.home.WareActivity" />
        <activity  android:name="com.naoh.Fruit.ui.AdminActivity"/>
        <activity android:name="com.naoh.Fruit.ui.photo.PublishActivity"></activity>
        <activity android:name="com.naoh.Fruit.ui.photo.GalleryActivity"></activity>
        <activity android:name="com.naoh.Fruit.ui.photo.AlbumActivity"></activity>
        <activity android:name="com.naoh.Fruit.ui.photo.ShowAllPhoto"></activity>
        <activity android:name="com.naoh.Fruit.util.photo.BitmapCache"></activity>
        <activity android:name="com.naoh.Fruit.ui.photo.ImageFile"></activity>
    </application>

</manifest>
